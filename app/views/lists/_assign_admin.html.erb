<% @list.users.each do |user| %>
  <%= user.name %> | <%= user.id %><br>
<% end %>

<%= form_for(@list) do |f| %>
  <%= f.fields_for :users, @list.users do |user_field| %>
    <%= user_field.hidden_field :id %>
    <%= user_field.object.name %>
    <%= user_field.check_box :enable %>
  <% end %>
  <%= f.submit %>
<% end %>